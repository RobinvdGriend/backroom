<h1>Profile</h1>
<table>
  <tr>
    <th>Username</th>
    <td><%= @user.name %></td>
  </tr>
</table>
<% if current_user == @user %>
  <%= link_to "Edit profile", edit_user_path %>
<% end %>

<% if current_user == @user %>
  <%= render template: "rooms/index" %>
<% else %>
  <span>You can't see other people's rooms.</span>
<% end %>

